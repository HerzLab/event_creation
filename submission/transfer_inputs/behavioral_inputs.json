{
  "r1": {
    "type": "group",
    "conversion": {
      "type": "group",
      "matlab_events": {
        "type": "file",
        "multiple": false,
        "required": true,
        "origin_directory": "{events_root}/RAM_{experiment}",
        "origin_file": "{code}_events.mat",
        "destination": "original_events.mat"
      },
      "verbal": {
        "type": "group",
        "math_events": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{events_root}/RAM_{experiment}",
          "origin_file": "{code}_math.mat",
          "destination": "math_events.mat"
        }
      },
      "contacts": {
        "type": "link",
        "multiple": false,
        "required": true,
        "origin_directory": "{db_root}/protocols/{protocol}/subjects/{subject}/localizations/{localization}/montages/{montage_num}/neuroradiology/current_processed",
        "origin_file": "contacts.json",
        "destination": "contacts.json"
      }
    },
    "transfer": {
      "type": "group",
      "system_1": {
        "type": "group",
        "eeg_log": {
          "type": "file",
          "multiple": false,
          "required": true,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}",
          "origin_file": "eeg.eeglog",
          "destination": "logs/eeg.eeglog"
        },
        "sync_pulses": {
          "type": "file",
          "multiple": false,
          "required": true,
          "origin_directory": "{data_root}/{code}/{sync_folder}/",
          "origin_file": "{sync_filename}",
          "destination": "logs/extracted_syncs.txt"
        },
        "PS": {
          "type": "group",
          "session_log": {
            "type": "file",
            "multiple": false,
            "required": true,
            "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}",
            "origin_file": "session.log",
            "destination": "logs/session.log"
          }
        }
      },
      "stim": {
        "type": "group",
        "contacts": {
          "type": "link",
          "multiple": false,
          "required": true,
          "origin_directory": "{db_root}/protocols/{protocol}/subjects/{subject}/localizations/{localization}/montages/{montage_num}/neuroradiology/current_processed",
          "origin_file": "contacts.json",
          "destination": "contacts.json"
        }
      },
      "default": {
        "type": "group",
        "eeg_sources": {
          "type": "link",
          "multiple": false,
          "required": true,
          "origin_directory": "{db_root}/protocols/{protocol}/subjects/{subject}/experiments/{new_experiment}/sessions/{session}/ephys/current_processed",
          "origin_file": "sources.json",
          "destination": "eeg_sources.json"
        }
      },
      "PS": {
        "type": "group",
        "system_2": {
          "type": "group",
          "host_logs": {
            "type": "file",
            "multiple": true,
            "required": true,
            "origin_directory": "{data_root}/{code}/raw/{experiment}_{original_session}",
            "origin_file": "log_*.log",
            "destination": "logs/host_logs/"
          }
        }
      },
      "TH": {
        "type": "group",
        "session_log": {
          "type": "file",
          "multiple": false,
          "required": true,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}",
          "origin_file": "{code}Log.txt",
          "destination": "logs/session_log.txt"
        },
        "eeg_log": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}",
          "origin_file": "eeg.eeglog.up",
          "destination": "logs/eeg.eeglog"
        }
      },
      "verbal": {
        "type": "group",
        "session_log": {
          "type": "file",
          "multiple": false,
          "required": true,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}",
          "origin_file": "session.log",
          "destination": "logs/session.log"
        },
        "math_log": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}/",
          "origin_file": "math.log",
          "destination": "logs/math.log"
        },
        "annotations": {
          "type": "file",
          "multiple": true,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/session_{original_session}",
          "origin_file": "*.ann",
          "destination": "annotations/"
        },
        "config": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}",
          "origin_file": "config.py",
          "destination": "experiment_files/config.py"
        },
        "sconfig": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}",
          "origin_file": "sconfig.py",
          "destination": "experiment_files/sconfig.py"
        }
      },
      "system_2": {
        "type": "group",
        "host_logs": {
          "type": "file",
          "multiple": true,
          "required": true,
          "origin_directory": "{data_root}/{code}/raw/{experiment}_{original_session}",
          "origin_file": "{experiment}_{subject}_*.log",
          "destination": "logs/host_logs/"
        }
      },
      "catFR": {
        "type": "group",
        "wordpool": {
          "type": "file",
          "multiple": false,
          "required": true,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}",
          "origin_file": "CatFR_WORDS.txt",
          "destination": "experiment_files/wordpool.txt"
        },
        "no_accent_wordpool": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}",
          "origin_file": "CatFR_WORDS_noAcc.txt",
          "destination": "experiment_files/wordpool_no_accent.txt"
        }
      },
      "FR": {
        "type": "group",
        "wordpool": {
          "type": "file",
          "multiple": false,
          "required": true,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/",
          "origin_file": "RAM_wordpool.txt",
          "destination": "experiment_files/wordpool.txt"
        },
        "no_accent_wordpool": {
          "type": "file",
          "multiple": false,
          "required": false,
          "origin_directory": "{data_root}/{code}/behavioral/{experiment}/",
          "origin_file": "RAM_wordpool_noAcc.txt",
          "destination": "experiment_files/wordpool_no_accent.txt"
        }
      }
    }
  },

  "ltp": {
    "type": "group",
    "ltpFR": {
      "type": "group",
      "wordpool": {
        "type": "file",
        "multiple": false,
        "required": true,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}/",
        "origin_file": "wasnorm_wordpool.txt",
        "destination": "experiment_files/wordpool.txt"
      },
      "eeg_log": {
        "type": "file",
        "multiple": true,
        "required": false,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}/session_{original_session}",
        "origin_file": "eeg.eeglog*",
        "destination": "logs/"
      },
      "eeg_params": {
        "type": "file",
        "multiple": false,
        "required": false,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}/session_{original_session}/eeg/eeg.noreref",
        "origin_file": "params.txt",
        "destination": "experiment_files/params.txt"
      }
    },
    "verbal": {
      "type": "group",
      "session_log": {
        "type": "file",
        "multiple": false,
        "required": true,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}/session_{original_session}",
        "origin_file": "session.log",
        "destination": "logs/session.log"
      },
      "math_log": {
        "type": "file",
        "multiple": false,
        "required": false,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}/session_{original_session}",
        "origin_file": "math.log",
        "destination": "logs/math.log"
      },
      "annotations": {
        "type": "file",
        "multiple": true,
        "required": false,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}/session_{original_session}",
        "origin_file": "*.ann",
        "destination": "annotations/"
      },
      "config": {
        "type": "file",
        "multiple": false,
        "required": false,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}",
        "origin_file": "config.py",
        "destination": "experiment_files/config.py"
      },
      "sconfig": {
        "type": "file",
        "multiple": false,
        "required": false,
        "origin_directory": "{data_root}/scalp/ltp/{experiment}/{code}",
        "origin_file": "sconfig.py",
        "destination": "experiment_files/sconfig.py"
      }
    }
  }
}